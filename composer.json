{
    "name": "webdriver-binary/webdriver-binary-downloader",
    "type": "library",
    "license": "MIT",
    "description": "General-issue library that enables building any kind of WebDriver binary downloader",
    "require": {
        "php": ">=7.2",
        "composer/composer": "^1.0 | ^2.0",
        "composer-plugin-api": "^1.0 | ^2.0"
    },
    "replace": {
        "vaimo/webdriver-binary-downloader": "*",
        "bolt/webdriver-binary-downloader": "*",
        "lanfest/webdriver-binary-downloader": "*"
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "support": {
        "source": "https://github.com/webdriver-binary/webdriver-binary-downloader",
        "docs": "https://github.com/webdriver-binary/webdriver-binary-downloader",
        "issues": "https://github.com/webdriver-binary/webdriver-binary-downloader/issues"
    },
    "keywords": [
        "browser driver",
        "webdriver",
        "downloader",
        "binary downloader",
        "binary"
    ],
    "autoload": {
        "psr-4": {
            "WebdriverBinary\\WebDriverBinaryDownloader\\": "src"
        }
    }
}
